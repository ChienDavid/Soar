sp {apply*move
   (state <s> ^operator <o>
              ^count <c>
              ^move <m>)
   (<o> ^name move
        ^direction <d>
        ^var <v>)
   (<s> ^<v> <old-value>)
   (<m> ^<d> {<new-value> <> <old-value>})
-->
   (<s> ^count <c> -
        ^count (+ <c> 1) +)
   (<s> ^<v> <old-value> -
        ^<v> <new-value> +)
}
