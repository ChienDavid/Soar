gp {cliff*propose*move
   (state <s> ^x <x>
              ^y <y>
              ^dist-to-cliff <c>
              ^dist-to-goal <g>)
-->
   (<s> ^operator <o> +)
   (<o> ^name move
        ^direction [north south east west]
        ^x <x>
        ^y <y>
        ^dist-to-cliff <c>
        ^dist-to-goal <g>)
}

sp {elaborate*move*var-x
   (state <s> ^operator <o> +)
   (<o> ^name move
        ^direction << east west >>)
-->
   (<o> ^var x)
}

sp {elaborate*move*var-y
   (state <s> ^operator <o> +)
   (<o> ^name move
        ^direction << north south >>)
-->
   (<o> ^var y)
}

sp {reject*move*wall
